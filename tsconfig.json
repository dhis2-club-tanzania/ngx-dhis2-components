{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "dhis2-dashboard": [
        "dist/dhis2-dashboard/dhis2-dashboard",
        "dist/dhis2-dashboard"
      ],
      "ngx-dhis2-dictionary": [
        "dist/ngx-dhis2-dictionary/ngx-dhis2-dictionary",
        "dist/ngx-dhis2-dictionary",
      ],
      "ngx-dhis2-http-client": [
        "dist/ngx-dhis2-http-client/ngx-dhis2-http-client",
        "dist/ngx-dhis2-http-client"
      ],
      "ngx-dhis2-org-unit-filter": [
        "dist/ngx-dhis2-org-unit-filter/ngx-dhis2-org-unit-filter",
        "dist/ngx-dhis2-org-unit-filter"
      ],
      "ngx-dhis2-menu": [
        "dist/ngx-dhis2-menu/ngx-dhis2-menu",
        "dist/ngx-dhis2-menu"
      ],
      "ngx-dhis2-period-filter": [
        "dist/ngx-dhis2-period-filter/ngx-dhis2-period-filter",
        "dist/ngx-dhis2-period-filter"
      ],
      "ngx-dhis2-data-filter": [
        "dist/ngx-dhis2-data-filter/ngx-dhis2-data-filter",
        "dist/ngx-dhis2-data-filter"
      ],
      "ngx-dhis2-selection-filters": [
        "dist/ngx-dhis2-selection-filters/ngx-dhis2-selection-filters",
        "dist/ngx-dhis2-selection-filters"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": true
  }
}
