{
  "name": "ngx-dhis2-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "npm run test -- --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:headless": "npm run e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "build-http-client": "ng build ngx-dhis2-http-client --prod",
    "build-org-unit-filter": "ng build ngx-dhis2-org-unit-filter --prod",
    "build-dictionary": "ng build ngx-dhis2-dictionary --prod",
    "build-data-filter": "ng build ngx-dhis2-data-filter --prod",
    "build-menu": "ng build ngx-dhis2-menu --prod",
    "build-period-filter": "ng build ngx-dhis2-period-filter --prod",
    "publish-http-client": "npm run build-http-client && cd dist/ngx-dhis2-http-client && npm publish --access public && cd ../../",
    "publish-org-unit": "npm run build-org-unit-filter && cd dist/ngx-dhis2-org-unit-filter && npm publish --access public && cd ../../",
    "publish-dictionary": "npm run build-dictionary && cd dist/ngx-dhis2-dictionary && npm publish --access public && cd ../../",
    "publish-data-filter": "npm run build-data-filter && cd dist/ngx-dhis2-data-filter && npm publish --access public && cd ../../",
    "publish-menu": "npm run build-menu && cd dist/ngx-dhis2-menu && npm publish --access public && cd ../../",
    "publish-period-filter": "npm run build-period-filter && cd dist/ngx-dhis2-period-filter && npm publish --access public && cd ../../",
    "pack-http-client": "npm run build-lib && cd dist/ngx-dhis2-http-client && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@iapps/function-analytics": "^1.0.0-beta.21",
    "@iapps/ngx-dhis2-http-client": "^2.0.8-rc.2",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/entity": "^9.0.0",
    "@ngrx/schematics": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "bootstrap": "^4.4.1",
    "dexie": "^3.0.0",
    "lodash": "^4.17.15",
    "ng2-dragula": "^2.1.1",
    "ngx-color-picker": "^9.1.0",
    "ngx-pagination": "^5.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.1",
    "codelyzer": "^5.2.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.0",
    "protractor": "~7.0.0",
    "terser": "^4.6.10",
    "ts-node": "~8.10.0",
    "tslint": "~6.1.0",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
